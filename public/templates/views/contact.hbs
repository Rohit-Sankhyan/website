{{>body}}
<!-- style-sheet -->
<link rel="stylesheet" href="/css/style.css">
<link rel="stylesheet" href="/css/contact.css">
</head>

<body onload="load1()" id="body">
<div class="ctn-l" id="loading">
  <span class="loader"></span>
</div>
  <!-- header section start -->

  <!-- /////////////////////////nav//////////////////////////////////////////// -->

  {{>navbar}}

  <!-- end of header section -->
  <!-- start of main section -->
  <main id="main">
    <!-- ======= Contact Section ======= -->
    <section id="contact" class="contact">
      <div class="container" data-aos="fade-up">
        <div class="section-title">
          <h1 class="text-center mt-5 txt fs-1">Contact</h1>
        
        </div>
        <iframe
          src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3403.3015151888!2d76.69085331507759!3d31.460890981389323!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x39053b269b5d8ebf%3A0x620b039aa358211e!2sSwami%20Vivekanand%20Government%20College!5e0!3m2!1sen!2sin!4v1675327805488!5m2!1sen!2sin"
          width="100%" height="450" style="border:0;" allowfullscreen="" loading="lazy"
          referrerpolicy="no-referrer-when-downgrade"></iframe>
        <div>
        </div>
          <br>
          <br>
          <br>
        <div class="row mt-5 md-5"data-aos="fade-right" data-aos-duration="2000">

          <div class="col-lg-4">
            <div class="info">
              <div class="address">
                <i class="fa-solid fa-location-dot"></i>
                <h4>Location:</h4>
                <p>Ghumarwin , Himachal Pradesh</p>
              </div>

              <div class="email">
                <i class="fa-solid fa-envelope"></i>
                <h4>Email:</h4>
                <p>rohitsharma162003@gmail.com</p>
              </div>

              <div class="phone">
               <i class="fa-solid fa-phone"></i>
                <h4>Call:</h4>
                <p>+91 9988556622</p>
              </div>

            </div>

          </div>

          <div class="col-lg-8 mt-5 mt-lg-0" data-aos="fade-left" data-aos-delay="500" data-aos-duration="2000">

            <form action="/contact" method="post" role="form" class="php-email-form">
              <div class="row">
                <div class="col-md-6 form-group">
                  <input type="text" name="name" class="form-control" id="name" placeholder="Your Name" required>
                </div>
                <div class="col-md-6 form-group mt-3 mt-md-0">
                  <input type="email" class="form-control" name="email" id="email" placeholder="Your Email" required>
                </div>
              </div>
              <div class="form-group mt-3">
                <input type="text" class="form-control" name="number" id="number" placeholder="Your Number" required>
              </div>
              <div class="form-group mt-3">
                <textarea class="form-control" name="message" rows="5" placeholder="Message" required></textarea>
              </div>
              <div class="text-center ">
                <br>
                <button class="btn fs-5 " type="submit"  >Send Message</button>
              </div>
              <br>
              
              </div>
            </form>

          </div>

        </div>

      </div>
    </section><!-- End Contact Section -->

  </main><!-- End #main -->


  <!-- end of main section -->
 

</body>

</html> {{>footer}}